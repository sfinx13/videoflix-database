basic:
	@echo '=========================================================================================';
	@echo 'SQL:'
	@echo '=========================================================================================';
	@cat sql/basic-level/$(request).sql ; echo;
	@echo '=========================================================================================';
	@echo 'RESULTS:'
	@echo '=========================================================================================';
	@docker exec -i videoflix_db bash -c 'mysql -u root -ppassword videoflix_db' < sql/basic-level/$(request).sql